package algo.binarysearch;

public class App {

	public static void main(String[] args) {
		int[] a = {1,7,3,9,5,};
		System.out.println(binaryfind(a,7));
	}
	
	public static int binaryfind(int[] a, int key) {
		int low = 0;
		int high = a.length-1;
		while(low<=high) {
			int middle = (low+high)/2;
			if(a[middle] == key) {
				return a[middle];
			}
			if(a[middle]>key) {
				low = middle - 1;
			}else {
				high = middle + 1;
			}
		}
		return -1;
	}

}
